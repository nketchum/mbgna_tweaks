    "require-dev": {
        "drupal/core-dev": "^10",
        "drupal/devel": "^5.0",
        "kint-php/kint": "^4.0",
        "mglaman/drupal-check": "^1.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "mglaman/composer-drupal-lenient": true,
            "wikimedia/composer-merge-plugin": true
        },
        "preferred-install": {
            "drupal/core": "dist"
        },
        "process": {
            "process-timeout": 0
        },
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "overwrite": true,
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": ".lando/assets/services/default.services.yml",
                "[web-root]/sites/default/default.services.local.yml": ".lando/assets/services/default.services.local.yml",
                "[web-root]/sites/default/default.services.prod.yml": ".lando/assets/services/default.services.prod.yml",
                "[web-root]/sites/default/default.services.stage.yml": ".lando/assets/services/default.services.stage.yml",
                "[web-root]/sites/default/default.settings.php": ".lando/assets/settings/default.settings.php",
                "[web-root]/sites/default/default.settings.local.php": ".lando/assets/settings/default.settings.local.php",
                "[web-root]/sites/default/default.settings.prod.php": ".lando/assets/settings/default.settings.prod.php",
                "[web-root]/sites/default/default.settings.stage.php": ".lando/assets/settings/default.settings.stage.php"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/auto_entitylabel": {
                "2945387 Support entities without bundles (User, etc.)": "https://www.drupal.org/files/issues/2022-05-20/2945387-19-support-bundleless-entities.patch"
            },
            "drupal/image_widget_crop": {
                "The Cropper library referenced in the README and in the libraries info is deprecated": "https://www.drupal.org/files/issues/2023-02-17/3093928-17.patch"
            },
            "drupal/core": {
                "2966656 Negotiate max width/height of oEmbed assets more intelligently": "https://www.drupal.org/files/issues/2023-01-31/2966656-34.patch"
            },
            "drupal/profile": {
                "Drupal 10.1: fatal error on account creation": "https://www.drupal.org/files/issues/2023-07-24/handle-user-is-anonymous-hardening-3371850-8.patch"
            },
            "drupal/commerce_ticketing": {
                "Drupal 10 upgrade": "https://raw.githubusercontent.com/nketchum/d10_contrib_patches/main/commerce_ticketing/commerce-ticketing-drupal10.patch"
            }
        },
        "patches-ignore": {},
        "composer-exit-on-patch-failure": false,
        "drupal-lenient": {
            "allowed-list": [
                "drupal/commerce_cybersource",
                "drupal/commerce_ticketing"
            ]
        }
    }
}
